{
  "name": "jahuty/jahuty-php",
  "description": "Jahuty's PHP SDK",
  "keywords": [
    "php",
    "jahuty"
  ],
  "license": "MIT",
  "homepage": "https://www.jahuty.com/",
  "authors": [
    {
      "name": "Jahuty and contributors",
      "homepage": "https://github.com/jahuty/jahuty-php/contributors"
    }
  ],
  "require": {
    "php": "^7.3 || ~8.0.0",
    "psr/http-factory": "^1",
    "psr/http-message": "^1",
    "psr/simple-cache": "^1",
    "guzzlehttp/guzzle": "^6.3 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^6",
    "donatj/mock-webserver": "^2.2",
    "ext-sockets": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Jahuty\\": "src/"
    }
  }
}
